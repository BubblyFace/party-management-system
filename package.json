{
  "name": "party-management-system",
  "private": true,
  "version": "1.0.0",
  "description": "党建管理系统 - 一个专为党委组织设计的综合管理平台",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "serve": "vite preview --port 4173",
    "start": "npm run dev"
  },
  "keywords": [
    "党建管理",
    "管理系统",
    "React",
    "TypeScript",
    "Ant Design",
    "Vite"
  ],
  "author": "Party Management System Team",
  "license": "MIT",
  "dependencies": {
    "@ant-design/charts": "^2.3.0",
    "@ant-design/icons": "^6.0.0",
    "@types/node": "^22.15.30",
    "antd": "^5.22.5",
    "dayjs": "^1.11.13",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.6.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/typography": "^0.5.16",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.17.0",
    "vite": "^6.0.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
